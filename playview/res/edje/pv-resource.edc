#define RESOURCE_IMAGE_LOSSY( FILE_NAME ) \
group { \
	name: FILE_NAME; \
	images.image: FILE_NAME LOSSY 85; \
	parts { \
		part { \
			name: "image"; \
			description { \
			state: "default" 0.0; \
			image.normal: FILE_NAME; \
			aspect: 1 1; \
			aspect_preference: BOTH; \
			} \
		} \
	} \
}

#define RESOURCE_IMAGE_COMP( FILE_NAME ) \
	group { \
		name: FILE_NAME; \
		images.image: FILE_NAME COMP; \
		parts { \
			part { name: "image"; \
				description { \
					state: "default" 0.0; \
					image.normal: FILE_NAME; \
					aspect_preference: VERTICAL; \
				} \
			} \
		} \
	}


